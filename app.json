{
	"expo": {
		"name": "redbee-expo",
		"slug": "redbee-expo",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "redbeeapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"associatedDomains": ["applinks:redbeeapp.com"],
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"ios": {
			"supportsTablet": true,
			"usesAppleSignIn": true,
			"bundleIdentifier": "com.redbeeapp.mobile",
			"buildNumber": "1",
			"entitlements": {
				"com.apple.developer.applesignin": ["Default", "Default"],
				"com.apple.developer.associated-domains": ["applinks:redbeeapp.com"],
				"com.apple.developer.in-app-payments": ["merchant.com.redbeeapp.mobile"]
			},
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptative-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.redbeeapp.mobile",
			"intentFilters": [
				{
					"action": "VIEW",
					"autoVerify": true,
					"data": [
						{
							"scheme": "https",
							"host": "redbeeapp.com"
						}
					],
					"category": ["BROWSABLE", "DEFAULT"]
				}
			]
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"@react-native-google-signin/google-signin",
				{
					"iosUrlScheme": "com.googleusercontent.apps.1063993481887-dc5u4v8ktj7c37acd6m97guj33faingm"
				}
			],
			"expo-apple-authentication",
			[
				"@stripe/stripe-react-native",
				{
					"merchantIdentifier": ["com.redbeeapp.mobile"],
					"enableGooglePay": true
				}
			],
			[
				"expo-video",
				{
					"supportsBackgroundPlayback": true,
					"supportsPictureInPicture": true
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "aaf6e6c1-c25b-4c04-a9b1-64d22f0cee3b"
			}
		}
	}
}
